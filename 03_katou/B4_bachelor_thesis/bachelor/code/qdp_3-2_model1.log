% (変数q_i_j) 各マスのクイーンの有無を表す変数
(int q_1_1 0 1)
(int q_1_2 0 1)
(int q_1_3 0 1)
(int q_2_1 0 1)
(int q_2_2 0 1)
(int q_2_3 0 1)
(int q_3_1 0 1)
(int q_3_2 0 1)
(int q_3_3 0 1)

% (制約3,4,5,6) 各行，各列，各右上がり対角線，各右下がり対角線のクイーンの有無を表す変数r_i，c_j，u_a，d_b
(int r_1 0 1)
(imp (> r_1 0) (> (+ q_1_1 q_1_2 q_1_3) 0))
(int r_2 0 1)
(imp (> r_2 0) (> (+ q_2_1 q_2_2 q_2_3) 0))
(int r_3 0 1)
(imp (> r_3 0) (> (+ q_3_1 q_3_2 q_3_3) 0))
(int c_1 0 1)
(imp (> c_1 0) (> (+ q_1_1 q_2_1 q_3_1) 0))
(int c_2 0 1)
(imp (> c_2 0) (> (+ q_1_2 q_2_2 q_3_2) 0))
(int c_3 0 1)
(imp (> c_3 0) (> (+ q_1_3 q_2_3 q_3_3) 0))
(int u_1 0 1)
(imp (> u_1 0) (> (+ q_1_1) 0))
(int u_2 0 1)
(imp (> u_2 0) (> (+ q_1_2 q_2_1) 0))
(int u_3 0 1)
(imp (> u_3 0) (> (+ q_1_3 q_2_2 q_3_1) 0))
(int u_4 0 1)
(imp (> u_4 0) (> (+ q_2_3 q_3_2) 0))
(int u_5 0 1)
(imp (> u_5 0) (> (+ q_3_3) 0))
(int d_1 0 1)
(imp (> d_1 0) (> (+ q_1_3) 0))
(int d_2 0 1)
(imp (> d_2 0) (> (+ q_1_2 q_2_3) 0))
(int d_3 0 1)
(imp (> d_3 0) (> (+ q_1_1 q_2_2 q_3_3) 0))
(int d_4 0 1)
(imp (> d_4 0) (> (+ q_2_1 q_3_2) 0))
(int d_5 0 1)
(imp (> d_5 0) (> (+ q_3_1) 0))

% (制約7) 各マスにクイーンが移動できるかを表す制約
(or (> r_1 0) (> c_1 0) (> u_1 0) (> d_3 0))
(or (> r_1 0) (> c_2 0) (> u_2 0) (> d_2 0))
(or (> r_1 0) (> c_3 0) (> u_3 0) (> d_1 0))
(or (> r_2 0) (> c_1 0) (> u_2 0) (> d_4 0))
(or (> r_2 0) (> c_2 0) (> u_3 0) (> d_3 0))
(or (> r_2 0) (> c_3 0) (> u_4 0) (> d_2 0))
(or (> r_3 0) (> c_1 0) (> u_3 0) (> d_5 0))
(or (> r_3 0) (> c_2 0) (> u_4 0) (> d_4 0))
(or (> r_3 0) (> c_3 0) (> u_5 0) (> d_3 0))

% (制約1) クイーングラフ上のクイーンの総数がkであることを意味する制約
(= (+ q_1_1 q_1_2 q_1_3 q_2_1 q_2_2 q_2_3 q_3_1 q_3_2 q_3_3) 2)
